<?xml version="1.0"?>
<robot name="my_robot">

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.6 0.4 0.1"/>
                <!-- <cylinder radius="0.3" length="0.1"/> -->
                <!-- <sphere radius="0.1"/> -->
            </geometry>
            <origin rpy="0.0 0 0" xyz="0 0 0"/> <!-- xoay theo roll pitch yaw-->
            <material name="acrylic">
            <color rgba="1 1 1 0.8"/>
            </material>
        </visual>
    </link>
<!-- wheel 1-->
    <link name="wheel1">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.03"/>
            </geometry>
            <origin rpy="1.6 0 0" xyz="0 0 0"/> <!-- xoay theo roll pitch yaw-->
            <material name="red">
            <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="joint1" type="continuous"> <!-- Tao lien ket giua 2 doi tuong-->
        <origin xyz="0.25 0.25 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="wheel1"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>
<!-- end wheel 1-->
<!-- wheel 2-->
    <link name="wheel2">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.03"/>
            </geometry>
            <origin rpy="1.6 0 0" xyz="0 0 0"/> <!-- xoay theo roll pitch yaw-->
            <material name="red">
            <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="joint2" type="continuous"> <!-- Tao lien ket giua 2 doi tuong-->
        <origin xyz="-0.25 0.25 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="wheel2"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>
<!-- end wheel 2-->
<!-- wheel 3-->
    <link name="wheel3">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.03"/>
            </geometry>
            <origin rpy="1.6 0 0" xyz="0 0 0"/> <!-- xoay theo roll pitch yaw-->
            <material name="red">
            <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="joint3" type="continuous"> <!-- Tao lien ket giua 2 doi tuong-->
        <origin xyz="0.25 -0.25 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="wheel3"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>
<!-- end wheel 3-->
<!-- wheel 4-->
    <link name="wheel4">
        <visual>
            <geometry>
                <cylinder radius="0.06" length="0.03"/>
            </geometry>
            <origin rpy="1.6 0 0" xyz="0 0 0"/> <!-- xoay theo roll pitch yaw-->
            <material name="red">
            <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="joint4" type="continuous"> <!-- Tao lien ket giua 2 doi tuong-->
        <origin xyz="-0.25 -0.25 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
